
page, .video{
    width: 100%;
    height: 100%;
}
.videoBox{
    width: 100%;
    height: 100%;
}
.goBack{
    width: 100%;
    height: 44px;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
        -ms-flex-align: center;
            align-items: center;
    position: absolute;
    top: -44px;
    left: 0;
    z-index: 2048;
    background: rgba(0, 0, 0, 0.5);
    -webkit-transition: top 0.5s;
    -o-transition: top 0.5s;
    transition: top 0.5s;
}
.goBack.show{
    top: 0;
}
.goBack>image{
    width: 20px;
    height: 20px;
    margin-left: 15px;
}
.playVideo{
    width: 100%;
    height: 100%;
}

