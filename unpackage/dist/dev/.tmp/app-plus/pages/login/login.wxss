
.login{
    width: 100%;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    -webkit-flex-direction: column;
        -ms-flex-direction: column;
            flex-direction: column;
    -webkit-box-align: center;
    -webkit-align-items: center;
        -ms-flex-align: center;
            align-items: center;
    background: #FFFFFF;
    margin-top: 130rpx;
}
.login input{
    width: 100%;
}
.login_logo{
    width: 114rpx;
    height: 129rpx;
}
.login_logo>image{
    width: 114rpx;
    height: 129rpx;
}
.login_mode{
    width: 580rpx;
    margin-top: 106rpx;
    margin-bottom: 30rpx;
    display: -webkit-inline-box;
    display: -webkit-inline-flex;
    display: -ms-inline-flexbox;
    display: inline-flex;
    border-bottom: 2px solid rgba(253,171,98,0.5);
}
.login_pwd{
    -webkit-box-flex: 1;
    -webkit-flex: 1;
        -ms-flex: 1;
            flex: 1;
    width: 50%;
    height: 56rpx;
    font-size: 30rpx;
    line-height: 52rpx;
    color: #FDAB62;
    text-align: center;
    opacity: 0.5;
}
.login_pwd.selsctClass{
    opacity: 1;
}
.login_pwd.selsctClass>text{
    display: inline-block;
    height: 54rpx;
    border-bottom:4rpx solid #FDAB62;
}
.login_code.selsctClass{
    opacity: 1;
}
.login_code.selsctClass>text{
    display: inline-block;
    height: 54rpx;
    border-bottom:4rpx solid #FDAB62;
}
.login_code{
    width: 50%;
    height: 56rpx;
    font-size: 30rpx;
    line-height: 52rpx;
    color: #FDAB62;
    text-align: center;
    opacity: 0.5;
}
.placeholder_style{
    font-size:28rpx;
    font-weight:400;
    color: #888888;
}
.login_family{
    width: 580rpx;
}
.swiper-item{
    width: 100%;
    height: 46rpx;
    margin-top: 72rpx;
    border-bottom: 1px solid #CECECE;
}
.login_btn{
    width:580rpx;
    height:80rpx;
    margin-top: 50rpx;
    line-height: 80rpx;
    text-align: center;
    color: #FFFFFF;
    font-weight:400;
    font-family:PingFangSC-Regular;
    background:#FDAB62;
    border-radius:8rpx;
    font-size:36rpx;
    opacity: 1;
}
